{
  "manifest_version": 3,
  "name": "Phishing Detection",
  "version": "1.0",
  "description": "Extensi√≥n para detectar y prevenir phishing",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "notifications",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>",
     "*://*/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  }
}


  